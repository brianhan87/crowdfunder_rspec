<!DOCTYPE html>
<html>
<head>
  <title>CrowdfunderRspec</title>
  <%= stylesheet_link_tag    "application", :media => "all" %>
  <%= javascript_include_tag "application" %>
  <%= csrf_meta_tags %>
</head>
<body>
	<%= render 'layouts/shared/nav' %>
	<div class="container">
		<% if flash[:notice] %>
			<div class="notice"><%= flash[:notice] %></div>
		<% end %>
		<% if flash[:alert] %>
			<div class="alert"><%= flash[:alert] %></div>
		<% end %>


		<%= yield %>		
	</div>



</body>
</html>
